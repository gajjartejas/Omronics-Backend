{
  "name": "omronics-backend",
  "version": "1.0.0",
  "description": "A starter Node.js API server using Express, with Docker, Jest, and ESLint. All in Typescript",
  "type": "module",
  "engines": {
    "node": ">= 18.12 <19"
  },
  "scripts": {
    "studio": "prisma studio",
    "migrate": "prisma migrate dev --name init",
    "start": "node dist/index.js",
    "start:prod": "node dist/index.js",
    "dev": "npm run build && concurrently -k \"tsc -w\" \"nodemon dist/index.js\"",
    "build": "npm i && prisma generate && npx tsc -p .",
    "test": "npm run build && jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-jwt": "^7.7.7",
    "jsonwebtoken": "^9.0.0",
    "nanoid": "^3.3.4"
  },
  "devDependencies": {
    "@types/http-errors": "^2.0.2",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.18.4",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "concurrently": "^8.2.1",
    "eslint": "~8.46",
    "eslint-config-prettier": "~9.0",
    "eslint-plugin-jest": "^27.4.2",
    "jest": "~29.6",
    "nodemon": "^2.0.20",
    "prettier": "~3.0",
    "prisma": "^4.7.1",
    "rimraf": "~5.0",
    "ts-jest": "^29.1.1",
    "typescript": "^4.9.5"
  }
}
