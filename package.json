{
  "name": "omronics-backend",
  "version": "1.0.0",
  "description": "A starter Node.js API server using Express, with Docker, Jest, and ESLint. All in Typescript",
  "main": "dist/index.js",
  "scripts": {
    "studio": "npx prisma studio",
    "migrate": "npx prisma migrate dev --name init",
    "start": "node dist/index.js",
    "start:prod": "node dist/index.js",
    "dev": "npm run build && concurrently -k \"tsc -w\" \"nodemon dist/index.js\"",
    "build": "npm i && prisma generate && npx tsc -p .",
    "test": "npm run build && jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "concurrently": "^7.6.0",
    "eslint": "8.22.0",
    "nodemon": "^2.0.20",
    "prettier": "2.8.8",
    "prisma": "^4.7.1",
    "rimraf": "^4.1.0",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "@types/http-errors": "^2.0.1",
    "@types/ioredis": "5.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-jwt": "^7.7.7",
    "http-errors": "^2.0.0",
    "ioredis": "5.3.2",
    "jsonwebtoken": "^9.0.0",
    "nanoid": "^3.3.4",
    "prisma-redis-middleware": "4.7.0"
  }
}
